{
  "presets": [
    "@babel/preset-react",
    [
      "@babel/preset-env",
      {
        "targets": {
          "browsers": [
            "last 2 versions",
            "safari >= 7",
            "ie > 10"
          ]
        }
      }
    ]
  ],
  "plugins": [
    "autobind-class-methods",
    "transform-class-properties",
    "@babel/plugin-proposal-object-rest-spread"
  ]
}