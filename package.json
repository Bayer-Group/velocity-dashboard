{
  "name": "velocity-dashboard",
  "version": "1.0.1",
  "license": "BSD-3-Clause",
  "scripts": {
    "start": "NODE_ENV=production node ./bin/www",
    "dev": "cross-env NODE_ENV=development node ./bin/www",
    "clean": "rimraf public/styles/style.css public/scripts/bundle.js public/scripts/bundle.js.map",
    "lessc": "cross-env lessc ./public/styles/style.less ./public/styles/style.css",
    "package": "cross-env NODE_ENV=production npm run lessc && webpack && cp ./node_modules/font-awesome/fonts/* ./dist/fonts/ && cp ./node_modules/weather-icons/font/* ./dist/fonts/ && cp public/styles/style.css dist/",
    "test": "mocha test",
    "test:dev": "mocha --watch test",
    "test:nyan": "mocha --watch --reporter nyan test"
  },
  "description": "",
  "repository": {
    "type": "git",
    "url": "git@github.com:MonsantoCo/velocity-dashboard.git"
  },
  "main": "/public/scripts/dashboard",
  "dependencies": {
    "compression": "~1.6.1",
    "express": "~4.13.4",
    "jade": "~1.11.0",
    "underscore": "~1.8.3",
    "coffee-script": "~1.10.0"
  },
  "devDependencies": {
    "chai": "~3.5.0",
    "chai-as-promised": "~5.3.0",
    "cjsx-loader": "~3.0.0",
    "coffee-loader": "~0.7.2",
    "coffee-react": "~5.0.0",
    "cross-env": "~1.0.8",
    "enzyme": "~2.3.0",
    "es6-promise": "~3.2.1",
    "font-awesome": "~4.6.3",
    "jsdom": "~8.5.0",
    "json-loader": "~0.5.4",
    "less": "~2.7.1",
    "less-middleware": "~2.1.0",
    "mocha": "~2.5.3",
    "moment": "~2.13.0",
    "piping": "~0.3.0",
    "proxyquire": "^1.7.9",
    "react": "~15.1.0",
    "react-addons-css-transition-group": "~15.1.0",
    "react-addons-test-utils": "~15.1.0",
    "react-component-width-mixin": "~2.0.0",
    "react-dom": "~15.1.0",
    "react-google-charts": "~1.0.2",
    "react-router": "~2.4.1",
    "rimraf": "~2.5.2",
    "sinon": "~1.17.4",
    "sinon-as-promised": "~4.0.0",
    "sinon-chai": "~2.8.0",
    "underscore": "~1.8.3",
    "weather-icons": "~1.3.2",
    "webpack": "~1.13.1",
    "webpack-dev-middleware": "~1.6.1"
  },
  "engines": {
    "node": "~4.4.4",
    "npm": "2.15.x"
  }
}
